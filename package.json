{
  "name": "node-react-cloudinary",
  "version": "1.0.0",
  "description": "A simple project to explain how to upload images to cloudinary in a node-react full stack app",
  "main": "index.js",
  "scripts": {
    "start": "npm run react:pro && babel-node ./server/server.js",
    "server:dev": "npm run react:pro && nodemon ./server/server.js --exec babel-node",     
    "react:dev": "webpack-dev-server --content-base src --inline --hot",
    "react:pro": "webpack"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cloudinary": "^1.9.1",
    "dotenv": "^4.0.0",    
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-dropzone": "^4.2.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  },
  "devDependencies": {
    "nodemon": "^1.12.1"
  },
  "author": "",
  "license": "ISC"
}
