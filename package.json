{
  "name": "node-react-cloudinary",
  "version": "1.0.0",
  "description": "A simple project to explain how to upload images to cloudinary in a node-react full stack app",
  "main": "webpack.config.js",
  "dependencies": {
    "axios": "^1.7.7",
